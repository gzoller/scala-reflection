package co.blocke.scala_reflection

import scala.quoted.*

object Liftables:

  given TypeSymbolToExpr: ToExpr[TypeSymbol] with {
    def apply(x: TypeSymbol)(using Quotes): Expr[TypeSymbol] = Expr(x.toString).asInstanceOf[Expr[TypeSymbol]]
  }

  given OptTypeSymbolToExpr: ToExpr[Option[TypeSymbol]] with {
    def apply(x: Option[TypeSymbol])(using Quotes): Expr[Option[TypeSymbol]] = 
      Expr(x.map(_.toString).asInstanceOf[Option[TypeSymbol]])
  }

  given TypedNameToExpr: ToExpr[Typed@@Name] with {
    def apply(x: TypedName)(using Quotes): Expr[TypedName] = Expr(x.toString.asInstanceOf[TypedName])
  }

  given TypedNameToExpr[T <: String]: ToExpr[T] with {
    def apply(x: T)(using Quotes) =
      import quotes.reflect._
      Literal(StringConstant(x)).asExpr.asInstanceOf[Expr[T]]
  }


